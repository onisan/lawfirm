@model MvcLawFirm.Models.NRBM_COUNSELS

@{
    ViewBag.Title = "Delete";
}

<h2>Delete Counsels Relation</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Counsels</legend>

    <div class="display-label">
         Lawyer
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NRBM_LAWYER.FNAME) @Html.DisplayFor(model => model.NRBM_LAWYER.LNAME)
    </div>

    <div class="display-label">
         Client
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NRBM_CLIENT.FNAME) @Html.DisplayFor(model => model.NRBM_CLIENT.LNAME)
    </div>

    <div class="display-label">
         Start Date
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.SDATE)
    </div>

    <div class="display-label">
         End Date
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EDATE)
    </div>

    <div class="display-label">
         Hours
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.HOURS)
    </div>

    <div class="display-label">
         Fees
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FEES)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}
