@model IEnumerable<MvcLawFirm.Models.NRBM_COUNSELS>

@{
    ViewBag.Title = "Results";
}

<h2>Counsel Search Results</h2>

<p>
    @Html.ActionLink("Create New", "Create") 
</p>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>

<table>
    <tr>
        <th>
            Lawyer
        </th>
        <th>
            Client
        </th>
        <th>
            Start Date
        </th>
        <th>
            End Date
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NRBM_LAWYER.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NRBM_CLIENT.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SDATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EDATE)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.COUNSELSID }) |
            @Html.ActionLink("Details", "Details", new { id=item.COUNSELSID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.COUNSELSID })
        </td>
    </tr>
}

</table>