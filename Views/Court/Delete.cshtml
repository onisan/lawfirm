@model MvcLawFirm.Models.NRBM_COURTAPPEARANCE

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Court Appearance</legend>
        @Html.HiddenFor(model=> model.CAID)
        @Html.HiddenFor(model=> model.CASEID)
    <div class="display-label">
         Client
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NRBM_CLIENT.FNAME) @Html.DisplayFor(model => model.NRBM_CLIENT.LNAME)
    </div>

    <div class="display-label">
         Lawyer
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NRBM_LAWYER.FNAME) @Html.DisplayFor(model => model.NRBM_LAWYER.LNAME)
    </div>

    <div class="display-label">
         Adverse Lawyer
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NRBM_ADVERSELAWYER.FNAME) @Html.DisplayFor(model => model.NRBM_ADVERSELAWYER.LNAME)
    </div>

    <div class="display-label">
         Court Date
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.DATEON)
    </div>

    <div class="display-label">
         Results
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.RESULTS)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}
